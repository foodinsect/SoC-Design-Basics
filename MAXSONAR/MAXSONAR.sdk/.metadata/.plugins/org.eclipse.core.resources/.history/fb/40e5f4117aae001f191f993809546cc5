  	#include "stdio.h"
	#include "xparameters.h"
	#include "mymaxsonar.h"

	void Delay(unsigned int n)
	{
		volatile unsigned int i;

		for (i=0; i<n; i++);
	}

	int main()
	{

		while (1){
			for (j=0; j<0x1000; j++){
				// Write value 'j' to the PWM IP core Reg
				MYPWM_mWriteReg(XPAR_MYMAXSONAR_0_S00_AXI_BASEADDR, 0);	// LED0
				MYPWM_mWriteReg(XPAR_MYMAXSONAR_0_S00_AXI_BASEADDR, 4);	// LED1
				MYPWM_mWriteReg(XPAR_MYMAXSONAR_0_S00_AXI_BASEADDR, 8);	// LED2

				Delay(2e4);

				printf("my Test 0x%x\n", j);
			}

		}

		return 0;
	}
