  	#include "stdio.h"
	#include "xparameters.h"
	#include "mymaxsonar.h"

	void Delay(unsigned int n)
	{
		volatile unsigned int i;

		for (i=0; i<n; i++);
	}

	int main()
	{

		while (1){
			MYMAXSONAR_mReadReg(XPAR_MYMAXSONAR_0_S00_AXI_BASEADDR, 0);	// LED0
			MYMAXSONAR_mReadReg(XPAR_MYMAXSONAR_0_S00_AXI_BASEADDR, 4);	// LED1
			MYMAXSONAR_mReadReg(XPAR_MYMAXSONAR_0_S00_AXI_BASEADDR, 8);	// LED2

			Delay(2e4);

			printf("my Test 0x%x\n");
		}

		return 0;
	}
